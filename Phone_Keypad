    private void b2MousePressed(java.awt.event.MouseEvent evt) {                                
        setVal(0);
        arr[0]++;
        String s = "";
        if(arr[0] == 1){
          s = a1.getText();
          if(s.length()>0) a1.setText(s.substring(0,s.length()));
          a1.append("A");
        }
        else if(arr[0] == 2){
           s = a1.getText();
           if(s.length()>0) a1.setText(s.substring(0,s.length()-1));
           a1.append("B");
        }
        else if(arr[0] == 3){
           s = a1.getText();
           if(s.length()>0)a1.setText(s.substring(0,s.length()-1));
           a1.append("C");
           setVal(111);
        }
        
        //a2.append(""+arr[0]);
    }                               

