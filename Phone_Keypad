    }                                  

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       try{
           possComb("",a1.getText()); 
       }
        catch(IOException e){}// TODO add your handling code here:
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        a2.append(l1.getSelectedValue()+" ");
        l1.setModel(new DefaultListModel());// TODO add your handling code here:
    }                                        

    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        a2.setText("");        // TODO add your handling code here:
    }                                        
    void setVal(int x)
    {
        for(int i=0;i<8;i++)
            if(i!=x) arr[i] = 0;
    }
    void possComb(String a, String p) throws IOException
    {
        DefaultListModel dm = (DefaultListModel) l1.getModel();
        int n = p.length();
        if(n==0){
           if(check_for_word(a)) dm.addElement(a);
        }
        else
        {
            for(int i=0;i<n;i++){
                l1.setModel(dm);
               possComb(a+p.charAt(i),p.substring(0,i)+p.substring(i+1,n));
            }
                
        }
    }
    public boolean check_for_word(String wor) throws IOException {
            BufferedReader in = new BufferedReader(new FileReader(
                    "Dictionary.txt"));
            String str;
            while ((str = in.readLine()) != null) {
                if (wor.equalsIgnoreCase(str)) 
                  return true;
            }
            in.close();
        return false;
    }

