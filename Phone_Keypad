    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        for(int i=0;i<8;i++)
             arr[i] = 0;
        if(a1.getText().length()>0) a1.setText(a1.getText().substring(0,a1.getText().length()-1)); 
        else JOptionPane.showMessageDialog(this,"No more letters to delete! Reached the limit!","Information",JOptionPane.INFORMATION_MESSAGE);// TODO add your handling code here:
    }                                        

